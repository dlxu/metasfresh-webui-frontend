    "classnames": "^2.2.5",
    "counterpart": "~0.18.0",
    "current-device": "^0.7.2",
    "d3": "4.10.0",
    "file-loader": "~1.1.11",
    "immutability-helper": "^2.1.2",
    "immutable": "^4.0.0-rc.9",
    "intro.js": "^2.7.0",
    "intro.js-react": "^0.1.5",
    "lodash": "^4.17.4",
    "moment": "~2.21.0",
    "numeral": "^2.0.6",
