    "redux-devtools": "~3.4.0",
    "redux-mock-store": "^1.5.1",
    "style-loader": "~0.20.3",
    "stylelint": "^7.9.0",
    "webpack": "~4.5.0",
    "webpack-dev-server": "~3.1.3",
    "webpack-node-externals": "^1.6.0",
    "why-did-you-update": "^0.1.1"
  },
  "dependencies": {
